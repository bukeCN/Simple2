<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.live.simple2">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ipc.client.ClientActivity">

        </activity>
        <activity android:name=".OtherActivity">



        </activity>

        <activity android:name=".animation.shareElement.StartActivity" />
        <activity android:name=".animation.shareElement.TargetActivity" />

        <service android:name=".ipc.server.RemoteService" android:process=":remote"/>
    </application>

</manifest>